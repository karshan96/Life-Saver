<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abc.life_saver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Search"
            android:label="@string/title_activity_search"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".BloodStock"
            android:label="@string/title_activity_blood_stock"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".ProfileUpdate"
            android:label="@string/title_activity_profile_update"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".UserRegister"
            android:label="@string/title_activity_user_register"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
    </application>

</manifest>